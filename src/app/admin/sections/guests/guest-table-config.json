{
    "search": {
        "placeholder": "Buscar por nombre",
        "property": "fullName",
        "min_chars": 3
    },
    "selects": [
        {
            "label": "Grupo",
            "name_property": "name",
            "value_property": "_id",
            "property_match": "invitation.group._id"
        },
        {
            "label": "Mesa",
            "name_property": "number",
            "value_property": "_id",
            "property_match": "table._id"
        }
    ],
    "new_element": {
        "label": "AÃ±adir invitado",
        "icon": "fa-user-plus"
    },
    "other_actions": [
        {
            "label": "Editar",
            "icon": "fa-pencil"
        },
        {
            "label": "Eliminar",
            "icon": "fa-trash"
        }
    ],
